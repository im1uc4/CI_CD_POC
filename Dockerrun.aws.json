{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
       {
      "name": "nserver",
      "image": "sushilisfine/mserver",
      "essential": true,
      "hostname":"server",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8761,
          "containerPort": 8761
        }
      ]
    },
    {
      "name": "ncore",
      "image": "sushilisfine/mcore",
      "essential": false,
      "hostname":"core",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8009,
          "containerPort": 8009
        }
      ],
      "links": [
        "nserver"
      ]
    },{
      "name": "nzuul",
      "image": "sushilisfine/mzuul",
      "hostname":"zuul",
      "memory": 128,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "links": [
        "nserver","ncore","nauth"
      ]
    },
    {
      "name": "nauth",
      "image": "sushilisfine/mauth",
      "hostname":"auth",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 9100,
          "containerPort": 9100
        }
      ],
      "links": [
        "nserver"
      ]
    },
	{
      "name": "nnginx",
      "image": "sushilisfine/mnginx",
      "hostname":"nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "nzuul"
      ]
    }
  ]
}
