{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
       {
      "name": "nameserver",
      "image": "sushilisfine/pocserver:1",
      "essential": true,
      "hostname":"server",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8761,
          "containerPort": 8761
        }
      ]
    },
    {
      "name": "namecore",
      "image": "sushilisfine/poccore:1",
      "essential": false,
      "hostname":"core",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8009,
          "containerPort": 8009
        }
      ],
      "links": [
        "nameserver"
      ]
    },{
      "name": "namezuul",
      "image": "sushilisfine/poczuul:1",
      "hostname":"zuul",
      "memory": 128,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "links": [
        "nameserver","namecore","nameauth"
      ]
    },
    {
      "name": "nameauth",
      "image": "sushilisfine/pocauth:1",
      "hostname":"auth",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 9100,
          "containerPort": 9100
        }
      ],
      "links": [
        "nameserver"
      ]
    },
    {
     "name": "namenginx",
     "image": "sushilisfine/pocnginx:1",
     "hostname":"nginx",
     "essential": true,
     "memory": 128,
     "portMappings": [
       {
         "hostPort": 80,
         "containerPort": 80
       }
     ],
     "links": [
       "namezuul"
     ]
   }
  ]
}
