{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
       {
      "name": "nameserver",
      "image": "sushilisfine/pocserver:4",
      "essential": true,
      "hostname":"pcserver",
      "memoryReservation": 1560,
      "memory": 2048,
      "portMappings": [
        {
          "hostPort": 8761,
          "containerPort": 8761
        }
      ]
    },
    {
      "name": "namecore",
      "image": "sushilisfine/poccore:4",
      "essential": false,
      "hostname":"pccore",
      "memoryReservation": 1560,
      "memory": 2048,
      "portMappings": [
        {
          "hostPort": 8009,
          "containerPort": 8009
        }
      ],
      "links": [
        "nameserver"
      ]
    },{
      "name": "namezuul",
      "image": "sushilisfine/poczuul:4",
      "hostname":"pczuul",
      "memoryReservation": 1560,
      "memory": 2048,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "links": [
        "nameserver","namecore","nameauth"
      ]
    },
    {
      "name": "nameauth",
      "image": "sushilisfine/pocauth:4",
      "hostname":"pcauth",
      "essential": true,
      "memoryReservation": 1560,
      "memory": 2048,
      "portMappings": [
        {
          "hostPort": 9100,
          "containerPort": 9100
        }
      ],
      "links": [
        "nameserver"
      ]
    },
    {
     "name": "namenginx",
     "image": "sushilisfine/pocnginx:4",
     "hostname":"pcnginx",
     "essential": true,
     "memoryReservation": 1560,
     "memory": 2048,
     "portMappings": [
       {
         "hostPort": 80,
         "containerPort": 80
       }
     ],
     "links": [
       "namezuul"
     ]
   }
  ]
}
