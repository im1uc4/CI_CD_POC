{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
       {
      "name": "nameserver",
      "image": "sushilisfine/pocserver",
      "essential": true,
      "hostname":"server",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8761,
          "containerPort": 8761
        }
      ]
    },
    {
      "name": "namecore",
      "image": "sushilisfine/poccore",
      "essential": false,
      "hostname":"core",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8009,
          "containerPort": 8009
        }
      ],
      "links": [
        "nserver"
      ]
    },{
      "name": "namezuul",
      "image": "sushilisfine/poczuul",
      "hostname":"zuul",
      "memory": 128,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "links": [
        "nameserver","namecore","nameauth"
      ]
    },
    {
      "name": "nameauth",
      "image": "sushilisfine/pocauth",
      "hostname":"auth",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 9100,
          "containerPort": 9100
        }
      ],
      "links": [
        "nameserver"
      ]
    },
	{
      "name": "namenginx",
      "image": "sushilisfine/pocnginx",
      "hostname":"nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "namezuul"
      ]
    }
  ]
}
